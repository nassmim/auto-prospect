{
	"mcpServers": {
		"filesystem": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"/home/user/auto-prospect"
			]
		},
		"next-devtools": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"next-devtools-mcp@latest"
			],
			"env": {}
		},
		"task-master-ai": {
			"type": "stdio",
			"command": "sh",
			"args": [
				"-c",
				"source .env.local 2>/dev/null || true && npx -y task-master-ai"
			],
			"env": {
				"TASK_MASTER_TOOLS": "standard"
			}
		}
	}
}
