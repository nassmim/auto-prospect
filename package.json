{
  "name": "auto-prospect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenvx run -- sh -c 'next dev --port ${NEXT_PUBLIC_PORT:-3000}'",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate-only": "dotenvx run --env-file=.env.development.local -- tsx scripts/migrate.ts",
    "db:migrate": "drizzle-kit generate && dotenvx run --env-file=.env.development.local -- tsx scripts/migrate.ts",
    "supabase:start": "npx dotenvx run --env-file=.env.local --env-file=.env.development --env-file=.env.development.local -- supabase start",
    "db:dump": "docker exec -it supabase_db_auto-prospect pg_dump -U postgres -d postgres --data-only --schema=auth --schema=public > supabase/seed.sql"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.2.0",
    "@next/env": "^16.1.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "jwt-decode": "^4.0.0",
    "libphonenumber-js": "^1.12.34",
    "next": "16.1.1",
    "postgres": "^3.4.8",
    "qrcode": "^1.5.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
