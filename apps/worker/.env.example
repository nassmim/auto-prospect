# Server configuration
PORT=3001

# Redis (required for BullMQ queues)
REDIS_URL=redis://localhost:6379

# API authentication (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
API_SECRET=your-secret-here

# Supabase (reuse from web app or get from Supabase dashboard)
SUPABASE_URL=your-supabase-url
SUPABASE_SERVICE_KEY=your-supabase-service-key

# Encryption keys for stored credentials
SMS_API_KEY_ENCRYPTION_KEY=your-32-byte-hex-key-here
